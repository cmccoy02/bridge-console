{
  "name": "bridge-console",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "concurrently \"npm run server\" \"npm run dev\" --names \"SERVER,VITE\" --prefix-colors \"blue,magenta\"",
    "test:api": "node test-api.js",
    "cleanup": "node cleanup-temp.js",
    "migrate": "node migrate-db.js",
    "reset-db": "rm -f bridge.sqlite && echo 'Database reset. Restart server to recreate.'"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "lucide-react": "^0.292.0",
    "madge": "^6.1.0",
    "npm-check-updates": "^16.14.6",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.9.0",
    "repomix": "^0.2.0",
    "simple-git": "^3.21.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.6",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^9.2.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "vite": "^5.0.0"
  }
}
